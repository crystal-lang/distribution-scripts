{
    "IndexDocument": {
        "Suffix": "index.html"
    },
    "ErrorDocument": {
        "Key": "api/${CRYSTAL_VERSION}/404.html"
    },
    "RoutingRules": [
        {
            "Condition": {
                "KeyPrefixEquals": "api/latest/${CRYSTAL_VERSION}/"
            },
            "Redirect": {
                "HostName": "crystal-lang.org",
                "HttpRedirectCode": "302",
                "Protocol": "https",
                "ReplaceKeyWith": "api/${CRYSTAL_VERSION}/404.html"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "api/latest/"
            },
            "Redirect": {
                "HostName": "crystal-lang.org",
                "HttpRedirectCode": "302",
                "Protocol": "https",
                "ReplaceKeyPrefixWith": "api/${CRYSTAL_VERSION}/"
            }
        },
        {
            "Condition": {
                "HttpErrorCodeReturnedEquals": "404",
                "KeyPrefixEquals": "api/"
            },
            "Redirect": {
                "HostName": "crystal-lang.org",
                "HttpRedirectCode": "301",
                "Protocol": "https",
                "ReplaceKeyPrefixWith": "api/latest/"
            }
        }
    ]
}
